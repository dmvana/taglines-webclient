<%= form_tag "/taglines", method: :post do %>
  <div>
    <%= label_tag :first_name %>
    <%= text_field_tag :first_name %>
  </div>
  <div>
    <%= label_tag :last_name %>
    <%= text_field_tag :last_name %>
  </div>
  <div>
    <%= label_tag :bs %>
    <%= text_field_tag :bs%>+
  </div>
  <div>
    <%= submit_tag "Submit" %>
  </div>
<% end %> 
